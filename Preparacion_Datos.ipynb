   "source": [
    "dataset['Peso'] = dataset['Peso'].astype(float) # Convertimos a float para poder calcular outliers\n",
    "dataset['Peso'] = dataset['Peso'].apply(lambda x: round(x, 2)) # Redondeamos a 1 decimal\n",
    "\n",
    "\n",
    "def remover_outlier(df, col):   # Función que remueve los valores fuera del los cuantiles 0.25 y 0.75\n",
    "  q1 = df[col].quantile(0.25)\n",
    "  q3 = df[col].quantile(0.75)\n",
    "  iqr = q3 - q1\n",
    "  lower_bound  = q1 - (1.5  * iqr)\n",
    "  upper_bound = q3 + (1.5 * iqr)\n",
    "  out_df = df.loc[(df[col] > lower_bound) & (df[col] < upper_bound)]\n",
    "  return out_df\n",
    "\n",
    "\n",
    "\n",
    "dataset = remover_outlier(dataset,'Peso')\n",
    "\n",
    "dataset.head(21)"
   ]
